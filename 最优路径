#include <iostream>
#include <cstring>
using namespace std;

int m, n;
int a[210][210];
int dp[210][210];
int f(int x, int y)
{
	if (dp[x][y] != -100000)
		return dp[x][y];
	int temp = -100000;
	int i, j;
	for (i = 1; i <= n; i++)
	{
		if (i == y)
			temp = max(temp, f(x - 1, i) + a[x][y] - a[x][y - 1]);
		else if (i < y)
			temp = max(temp, a[x][y] - a[x][i - 1] + f(x - 1, i));
		else if (i > y)
			temp = max(temp, -a[x][y - 1] + a[x][i] + f(x - 1, i));
	}
	dp[x][y] = temp;
	return dp[x][y];
}
int main()
{
	int i, j, k;
	while (cin >> m >> n)
	{
		memset(a, 0, sizeof(a));
		for (i = 1; i <= m; i++)
			for (j = 1; j <= n; j++)
			{
				cin >> a[i][j];
				a[i][j] += a[i][j - 1];//前缀和
			}

		int ans = -100000;
		for (i = 1; i <= n; i++)//对第一行每一个做dp
		{
			memset(dp, 0, sizeof(dp));
			dp[1][i] = a[1][i] - a[1][i - 1];
			for (j = i - 1; j >= 1; j--)
				dp[1][j] = a[1][i] - a[1][j - 1];
			for (j = i + 1; j <= n; j++)
				dp[1][j] = a[1][j] - a[1][i - 1];
			for (j = 2; j <= m; j++)
				for (k = 1; k <= n; k++)
					dp[j][k] = -100000;
			for (j = 1; j <= n; j++)
				ans = max(ans, f(m, j));
		}
		cout << ans << endl;
	}
}
